<div class="row" id="images_row">
    <div class="col-md-12">
        <h2>Images</h2>
        <hr />
    </div>
    <div class="col-md-4">
        {{#each images}}
        <img src="/images/uploaded/{{this}}" class="img-responsive img-thumbnail" />
        {{else}}
        <strong>No Images Available</strong>
        {{/each}}
    </div>
</div>